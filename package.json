{
  "name": "outscraper-app",
  "version": "1.0.0",
  "description": "Google Maps Places Data Fetcher using Outscraper",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-views",
    "copy-views": "cp -r src/views dist/views",
    "watch": "tsc -w"
  },
